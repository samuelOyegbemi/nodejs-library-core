{
  "name": "@wbc-nodejs/core",
  "version": "1.0.0",
  "description": "Core Nodejs library for Webcoupers",
  "main": "build/index.js",
  "types": "build/types.d.ts",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "yarn clean && yarn build-babel",
    "build-babel": "babel ./src --out-dir build",
    "clean": "rm -rf build && mkdir build",
    "prepublishOnly": "yarn build && jsdoc -t node_modules/tsd-jsdoc/dist -r ./src -d build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/webcoupers_/nodejs-core-library.git"
  },
  "keywords": [
    "wbc",
    "webcoupers",
    "shared",
    "nodejs",
    "library"
  ],
  "author": "Webcoupers Engineering",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "tsd-jsdoc": "^2.5.0"
  },
  "dependencies": {
    "cloudinary": "^1.23.0",
    "datauri": "^2.0.0",
    "dotenv": "^8.2.0",
    "express-validator": "^6.9.2",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "winston": "^3.3.3"
  }
}
