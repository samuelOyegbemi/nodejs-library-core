{
  "name": "@wbc-nodejs/core",
  "version": "2.0.2",
  "description": "Core Nodejs library for Webcoupers",
  "main": "build/index.js",
  "types": "build/types",
  "files": [
    "build/**/*"
  ],
  "exports": {
    ".": {
      "node": {
        "module": "build/index.js"
      }
    }
  },
  "module": "build/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf build && mkdir build",
    "build": "yarn clean && yarn build-babel",
    "build-babel": "babel ./src --out-dir build",
    "prepublishOnly": "yarn build && jsdoc -t node_modules/tsd-jsdoc/dist -r ./src -d build/types && mv build/types/types.d.ts build/types/index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/webcoupers_/nodejs-core-library.git"
  },
  "keywords": [
    "wbc",
    "webcoupers",
    "shared",
    "nodejs",
    "library"
  ],
  "author": "Webcoupers Engineering",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "tsd-jsdoc": "^2.5.0"
  },
  "dependencies": {
    "cloudinary": "^1.23.0",
    "datauri": "^2.0.0",
    "dotenv": "^8.2.0",
    "express-validator": "^6.9.2",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "winston": "^3.3.3"
  }
}
